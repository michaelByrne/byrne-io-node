<body>


<!-- Ugh, get this garbage out of here!-->
<script>
    $(document).ready(function () {
        $('#tlogo').hover(function () {
            this.src = '/images/twitterover.png';
        }, function () {
            this.src = '/images/twitter.png';
        });
    });
</script>

<div class="container">
    <div class="page-header row" id="top-bar"><div class="col-md-3"><h2>Michael Byrne</h2></div><div class="col-md-4"><img src="/images/byrneio1.jpg" class="img-responsive"></div> </div>
</div>
<div class="row">
    <div class="navbar navbar-default col-md-1">
        <ul class="nav nav-stacked navbar-nav">
            <li id="front"><a href="<%= nav[0].Link %>"><%= nav[0].Text %></a></li>
            <li class="active"><a href="<%= nav[1].Link %>"><%= nav[1].Text %></a></li>
            <li><a href="<%= nav[2].Link %>"><%= nav[2].Text %></a></li>
            <li class="not-active"><a href="<%= nav[3].Link %>"><%= nav[3].Text %></a></li>
            <li class="not-active"><a href="<%= nav[4].Link %>"><%= nav[4].Text %></a></li>
        </ul>
        <div id="twitterOff" style="padding-left: 10px"><a href="https://twitter.com/everydayelk"><img id="tlogo" width="50" src="/images/twitter.png"/></a></div>
    </div>
    <%if (stories.length > 0) { %>
    <% for(var i = 0;i < stories.length;i++){ %>
    <div class="col-md-3 story">
        <div class="story-content">
            <a href="<%=stories[i].url%>" target="_blank"><img class="crop story-lead" src=<%="../images/uploads/" + stories[i].img%>></a>
            <h6 style="color: #58889D"><%= stories[i].tag %></h6>
            <div class="story-title"><a href="<%=stories[i].url%>" target="_blank" style="color: darkgray"><h4><%= stories[i].title %></h4></a></div>
            <div class="blurb"><%=stories[i].blurb%></div>
        </div>
    </div>
    <% } %>
    <% } %>
</div>


<div id="push"></div>

<div id="footer">
    <div class="container" style="text-align: center">
        <p class="muted credit">tkms v 0.1 okokokok copyright 2016 mbyrne</p>
    </div>
</div>

</body>