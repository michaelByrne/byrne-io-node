<script>
    $(document).ready(function () {
        $('#tlogo').hover(function () {
            this.src = '/images/twitterover.png';
        }, function () {
            this.src = '/images/twitter.png';
        });

    });
</script>

<div class="container">
    <div class="page-header"><h2>Michael Byrne</h2></div>
</div>
<div class="row">
    <div class="navbar navbar-default col-md-1">
        <ul class="nav nav-stacked navbar-nav">
            <li id="front"><a href="<%= nav[0].Link %>"><%= nav[0].Text %></a></li>
            <li><a href="<%= nav[1].Link %>"><%= nav[1].Text %></a></li>
            <li class="active"><a href="<%= nav[2].Link %>"><%= nav[2].Text %></a></li>
            <li class="not-active"><a href="<%= nav[3].Link %>"><%= nav[3].Text %></a></li>
            <li id="twitterOff"><a href="#"><img id="tlogo" width="50" src="/images/twitter.png"/></a></li>
        </ul>
    </div>
    <div class="col-md-1" style="width: 30px">
        <div class="btn-toolbar" role="toolbar" aria-label="Toolbar with button groups" style="margin-left: 5px">
            <div class="btn-group-vertical" role="group" aria-label="First group" id="btns">
                <% for(var i = 1;i <= images.length;i++){ %>
                <button type="button" class="btn btn-secondary" id="img-btn-<%=i-1%>"><%=i%></button>
                <script>
                    $('#img-btn-0').addClass('active');
                    $('#img-btn-1').addClass('active');
                    $('#btns').on('click','#img-btn-<%=i-1%>',function(){
                        $("#img-1").attr('src', '<%= images[i - 1].url %>');
                        <%=images[i-1].active=true%>
                        $("#img-btn-<%= i-1 %>").addClass('active');
                    });
                </script>
                <% } %>
            </div>
        </div>
    </div>

    <div class="col-md-5">
        <img src="<%=images[0].url%>" class="img-responsive img-one" id="img-<%=1%>">
    </div>
    <div class="col-md-5">
        <img src="<%=images[1].url%>" class="img-responsive img-two" id="img-<%=2%>">
    </div>




</div>

<div id="push"></div>

